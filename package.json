{
  "name": "fe-inventory",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "npm-i": "npm i",
    "npm-update": "npm update",
    "json-server": "npm install -g json-server",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "watch": "npx tailwindcss -i ./src/styles/tailwind-input.css -o ./src/styles/tailwind.css --watch",
    "minify": "npx tailwindcss -i ./src/styles/tailwind-input.css -o ./src/styles/tailwind.css --minify",
    "userdata": "json-server -w ./src/api/userdata.json -p 5001",
    "categorydata": "json-server -w ./src/api/categorydata.json -p 5002",
    "stockdata": "json-server -w ./src/api/stockdata.json -p 5003",
    "stockindata": "json-server -w ./src/api/stockindata.json -p 5004",
    "stockoutdata": "json-server -w ./src/api/stockoutdata.json -p 5005"
  },
  "dependencies": {
    "autoprefixer": "10.4.15",
    "axios": "^1.4.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "next": "13.4.19",
    "postcss": "8.4.28",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1",
    "sharp": "^0.32.1",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0"
  }
}